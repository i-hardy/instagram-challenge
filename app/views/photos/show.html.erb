<section class="show-photo-view">
  <section class="show-photo-box">
    <%= image_tag @photo.image.url %>
  </section>
  <section class="show-photo-box">
    <%= link_to profile_path(@photo.user) do %>
    <h3>@<%= @photo.user.username %></h3>
    <% end %>
    <p><%= @photo.description %></p>
  </section>
</section>
