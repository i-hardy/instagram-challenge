<h3>Upload a photo to Ynstagram</h3>
<section class="photo-upload-form">
  <%= form_for @photo, html: { multipart: true }, url: photos_path do |f| %>
    <div class="description">
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </div>
    <div class="field">
      <%= f.label :image %><br>
      <%= f.file_field :image %>
    </div><br>
    <div class="actions">
      <%= f.submit 'Upload' %>
    </div>
  <% end %>
</section>
